{
    "name": "Notebox",
    "description": "Take quick notes in Chrome using this simple notepad",
    "version": "1.0",
    "manifest_version": 3,
    "options_page": "options.html",

    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "/images/nb_icon482.png"
        }
    },

    "permissions": ["storage"]
}